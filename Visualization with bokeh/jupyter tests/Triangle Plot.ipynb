    "from bokeh.plotting import figure\n",
    "from bokeh.io import output_file, show\n",
    "\n",
    "# Triangle points\n",
    "x1 = [3, 7.5, 10]\n",
    "y1 = [3, 6, 9]\n",
    "\n",
    "#prepare output file\n",
    "output_file(\"Triangle.html\")\n",
    "\n",
    "f2 = figure()\n",
    "f2.triangle(x1,y1)\n",
    "show(f2)"
   ]
  }
